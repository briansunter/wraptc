{
  "name": "@wrap-terminalcoder/core",
  "version": "1.0.0",
  "description": "Core integration and routing logic for wrap-terminalcoder",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bun build src/index.ts --outdir=dist --target=node",
    "test": "bun test",
    "test:unit": "bun test src/__tests__/unit/router.test.ts src/__tests__/unit/state.test.ts src/__tests__/unit/config.test.ts src/__tests__/unit/memory-monitor.test.ts",
    "test:integration": "bun test src/__tests__/integration/",
    "test:smoke": "bun test src/basic-test.test.ts",
    "test:coverage": "bun test --coverage",
    "test:coverage:unit": "bun test src/__tests__/unit/ --coverage",
    "test:coverage:integration": "bun test src/__tests__/integration/ --coverage",
    "test:watch": "bun test --watch"
  },
  "keywords": ["terminal", "ai", "routing", "provider", "integration"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
