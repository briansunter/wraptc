{
  "routing": {
    "defaultOrder": ["user-provider", "system-provider"]
  },
  "providers": {
    "user-provider": {
      "binary": "/usr/local/bin/user-provider",
      "args": ["--user-flag"],
      "jsonMode": "flag",
      "jsonFlag": "--json",
      "streamingMode": "jsonl",
      "capabilities": ["generate", "edit"]
    }
  }
}
